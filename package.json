{
  "name": "dark-icon-generator",
  "version": "1.0.3",
  "description": "",
  "main": "lib/index.js",
  "module": "lib/esm/index.js",
  "browser": "lib/esm/browser.js",
  "types": "lib/index.d.ts",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "require": "./lib/index.js",
      "import": "./lib/esm/index.js",
      "browser": "./lib/esm/browser.js",
      "default": "./lib/index.js"
    },
    "./browser": {
      "types": "./lib/browser.d.ts",
      "require": "./lib/browser.js",
      "import": "./lib/esm/browser.js"
    }
  },
  "scripts": {
    "build": "npm run build:cjs && npm run build:esm",
    "build:cjs": "tsc -p tsconfig.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "demo:prepare": "npm run build && node scripts/prepare-browser-demo.js",
    "test": "jest --config jestconfig.json",
    "lint": "eslint .",
    "prepare": "npm run build",
    "prepublishOnly": "npm run test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run lint && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "files": [
    "lib/**/*"
  ],
  "keywords": [],
  "author": "agoudbg <agoudbg@gmail.com>",
  "homepage": "https://github.com/agoudbg/Dark-Icon-Generator",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.7.9",
    "eslint": "^9.13.0",
    "globals": "^15.11.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.11.0"
  },
  "dependencies": {
    "canvas": "^2.11.2"
  }
}